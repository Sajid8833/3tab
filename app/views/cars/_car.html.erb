<div id="<%= dom_id car %>">


  <p>
    <strong>Name:</strong>
    <%= car.name %>
  </p>

  <p>
    <strong>Model:</strong>
    <%= car.model %>
  </p>

  <p>
    <strong>Detail:</strong>
    <%= car.details %>
  </p>

  <p>
    <strong>Seller name:</strong>
    <%= car.seller_name %>
  </p>

  <p>
    <strong>Issue year:</strong>
    <%= car.issue_year %>
  </p>

  <p>
    <strong>Condition:</strong>
    <%= car.condition %>
  </p>

  <p>
    <strong>image:</strong>
    <%= car.image %>
